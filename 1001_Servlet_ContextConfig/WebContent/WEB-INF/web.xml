<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
         xmlns="http://xmlns.jcp.org/xml/ns/javaee" 
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>1001_Servlet_ContextConfig</display-name>
  
  <servlet>
      <servlet-name>akhilesh</servlet-name>
      <servlet-class>com.servlet.MyServlet1</servlet-class>
      
      <init-param>
          <param-name>tech</param-name>
          <param-value>Hibernate</param-value>     
      </init-param>
      
  </servlet>
  
  <servlet-mapping>
       <servlet-name>akhilesh</servlet-name>
       <url-pattern>/request1</url-pattern>  
  </servlet-mapping>
  
  
  
  
  
  <servlet>
      <servlet-name>kumar</servlet-name>
      <servlet-class>com.servlet.MyServlet2</servlet-class>
      
  </servlet>
  
  <servlet-mapping>
       <servlet-name>kumar</servlet-name>
       <url-pattern>/request2</url-pattern>  
  </servlet-mapping>
    
  
  <context-param>
      <param-name>tech</param-name>
      <param-value>Java</param-value> 
  </context-param>
  
  <context-param>
      <param-name>phone</param-name>
      <param-value>IPhone XR</param-value>
  </context-param>
  
  
</web-app>